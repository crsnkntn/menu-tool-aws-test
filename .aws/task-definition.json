{
    "family": "flask-task",
    "containerDefinitions": [
      {
        "name": "flask-container",
        "image": "menu-tool.dkr.ecr.us-east-2.amazonaws.com/menu-tool:latest",
        "memory": 4,
        "cpu": 1,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 5000,
            "hostPort": 5000,
            "protocol": "tcp"
          }
        ]
      }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "networkMode": "awsvpc",
    "cpu": "1",
    "memory": "4",
    "executionRoleArn": "arn:aws:iam::menu-tool:role/ecsTaskExecutionRole"
  }
  